Agent Controller
	Lobsvars = {sI, b, s1, l1, l2, r1, s4, r2, s5, g, s6, s7};
	Vars : 
		imp_s2_s3: boolean;
		play : boolean;
	end Vars
	Actions = {l,r,a,e,o,i};
	Protocol : 
		Environment.s3 = true and Environment.r1 = true : {o};
		Environment.s4 = true and Environment.r1 = true and Environment.r2 = true : {a,o};
		Environment.s5 = true and Environment.g = true : {l,r,i,e,a,o};
		Environment.s6 = true and Environment.l2 = true : {o};
		Environment.s7 = true and Environment.r2 = true : {o};
		Environment.sI = true and Environment.b = true : {r,l};
		Environment.s1 = true and Environment.l1 = true and Environment.l2 = true : {a,o};
		Environment.s2 = true and Environment.l1 = true : {o};
	end Protocol
	Evolution : 
		play = true if play = true;
		(imp_s2_s3 = true ) if (Environment.sI = true )and (Environment.b = true )and (imp_s2_s3 = false )and (Environment.l1 = false )and (Environment.r1 = false )and (Train1.Action = l and Train2.Action = r and Controller.Action = l);
		(imp_s2_s3 = true ) if (Environment.sI = true )and (Environment.b = true )and (imp_s2_s3 = false )and (Environment.l1 = false )and (Environment.r1 = false )and (Train1.Action = r and Train2.Action = l and Controller.Action = r);
	end Evolution
end Agent
Evaluation
	l1 if (Environment.l1 = true);
	b if (Environment.b = true);
	g if (Environment.g = true);
	end Evaluation
	InitStates
		Environment.sI = true and 
		Environment.s1 = false and 
		Environment.s2 = false and 
		Environment.s3 = false and 
		Environment.s4 = false and 
		Environment.s5 = false and 
		Environment.s6 = false and 
		Environment.s7 = false and 
		Train1.imp_s1_s2 = false and 
		Train1.imp_s3_s4 = false and 
		Controller.imp_s2_s3 = false and 
		Environment.r2 = false and 
		Environment.b = true and 
		Environment.l1 = false and 
		Environment.l2 = false and 
		Environment.g = false and 
		Environment.r1 = false;
	end InitStates
Groups
	g1={Train1,Controller};
	g2={Controller};
end Groups
Formulae
	<g1>X((l1 and <g1>X((!(b) or <g1>F(g)))));
end Formulae
